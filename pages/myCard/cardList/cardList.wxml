

<view class="card-container container">
  <view class="card-cell" wx:for="{{couponList}}" wx:key="index" data-num="{{item.name}}" bindtap="linkTo">
    <image class="bg-image" src="../../../images/bg1.png"></image>
    <view class="card-left">
      <image src="../../../images/card.png"></image>
    </view>
    <view class="card-right">
      <view class="big-txt">{{item.name}}</view>
      <view class="small-txt">
        <text>有效期：</text>
        <text class="red-color">{{item.validPeriod}}</text>
      </view>
    </view>
  </view>
</view>